<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/nen"
    tools:context=".SiingInActivity">

    <EditText
        android:id="@+id/editTextdangkiEmail"
        android:layout_width="339dp"
        android:layout_height="43dp"
        android:layout_marginTop="276dp"
        android:ems="10"
        android:hint="email"
        android:background="@drawable/cuttomeditdangnhhap"
        android:paddingLeft="17dp"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.383"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextdangkiPassword"
        android:layout_width="343dp"
        android:layout_height="41dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:paddingLeft="17dp"
        android:hint="Password"
        android:background="@drawable/cuttomeditdangnhhap"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextdangkiEmail" />

    <Button
        android:id="@+id/buttonxacnhandangki"
        android:layout_width="340dp"
        android:layout_height="41dp"
        android:layout_marginTop="44dp"
        android:background="@drawable/cuttomeditdangnhhap"
        android:text="Sign UP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextdangkiPassword" />


</androidx.constraintlayout.widget.ConstraintLayout>